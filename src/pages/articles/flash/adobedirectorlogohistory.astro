---
import DefaultBody from '../../../layouts/DefaultBody.astro';
import Navbar from '../../../layouts/articles/Navbar.astro';
import Scrollspy from '../../../layouts/articles/Scrollspy.astro';
import BackToTop from '../../../components/BackToTop.astro';
---

<DefaultBody title="Adobe Director Logo History" icon="Adobe_Flash_Player_32.svg.png" color="#4a0000" description="Flash Documentation presents 'Adobe Director Logo History'!" author="AT Products LLC & Flash Documentation" scrollspy="true" />
<div id="page">
  <Navbar pageTitle="Adobe Director Logo History" 
    link={[
     { name: "Flash Documentation" , href: "/flash" },
    ]}
  />
  <div id="main-content">
    <Scrollspy id="dl" 
      scrollcontent={[ 
        { name: "Macromind Videoworks", href: "#mvtitle", additionalClasses: "fw-bolder" },
        { name: "Version 1 (1985)", href: "#1985" },
        { name: "Version 2 (1987)", href: "#1987" },
        { name: "Macromind Director", href: "#mdtitle", additionalClasses: "fw-bolder" },
        { name: "Version 1 (1989)", href: "#1989" },
        { name: "Version 2 (1990)", href: "#1990" },
        { name: "Version 3 (1991)", href: "#1991" },
        { name: "Version 3.1 (1992)", href: "#1992" },
        { name: "Macromedia Director", href: "#matitle", additionalClasses: "fw-bolder" },
        { name: "Version 3.1.3 (1993)", href: "#1993" },
        { name: "Version 4 (1994)", href: "#1994" },
        { name: "Version 5 (1996)", href: "#1996" },
        { name: "Version 6 (1997)", href: "#1997" },
        { name: "Version 7 (1998)", href: "#1998" },
        { name: "Version 8 (2000)", href: "#2000" },
        { name: "Version 8.5 (2001)", href: "#2001" },
        { name: "Versions 9 - 10 (2002)", href: "#2002" },
        { name: "Adobe Director", href: "#aetitle", additionalClasses: "fw-bolder" },
        { name: "Version 11 (2007)", href: "#2007" },
        { name: "Version 11.5 (2008)", href: "#2008" },
        { name: "Version 12 (2010)", href: "#2010" },
      ]} 
    >
      <Fragment slot="scrollspy-content">
        <div id="intro" class="story-top-scroll">
          <h1 class="text-start">Adobe Director Logo History</h1>
          <h5 class="text-start"><b>Featured In:</b> <a href="/flash">Flash Documentation</a>.</h5>
          <h5 class="text-start"><b>Made & Published by:</b> <a href="/">AT Products LLC</a>.</h5>
          <hr>
        </div>
        <div id="content">
          <div id="videoworks">
            <h2 id="mvtitle"> Macromind Videoworks </h2>
            <div id="1985">
              <h3> 1985 - 1987 </h3>
              <h4> Version 1 </h4>
              <img src="/media/images/MacromindVideoworks_1.webp" width="200" height="200" alt="1985-1987 | version 1"> <br> <br>
              <img src="/media/images/bannerMacromindVideoworks_1.webp" alt=" 1985-1987 | version 1">
            </div>
            <br>
            <div id="1987">
              <h3> 1987 - 1989 </h3>
              <h4> Version 2 </h4>
              <img src="/media/images/MacromediaVideoworks_2.webp" width="200" height="200" alt="1987-1989 | version 2">
            </div>
          </div>
          <hr>
          <div id="director">
            <h2 id="mdtitle"> Macromind Director </h2>
            <div id="1989">
              <h3> 1989 - 1990 </h3>
              <h4> Version 1 </h4>
              <img src="/media/images/MacromindDirector_1.webp" width="200" height="137" alt="1989-1990 | version 1">
            </div>
            <br>
            <div id="1990">
              <h3> 1990 - 1991 </h3>
              <h4> Version 2 </h4>
              <img src="/media/images/MacromindDirector_2.webp" width="200" height="205" alt="1990-1991 | version 2">
