      body, html {
        height: 100vh;
      }

      body {
        overflow-y: auto;
      }

      .back-to-top-alt {
        box-shadow: 0 0.5rem 1rem rgba(var(--bs-body-color-rgb), 0.15) !important;
        color: white;
        background-color: black;
      }

      .back-to-top-alt:hover {
        color: white;
        background-color: #262626;
      }

      .back-to-top-alt:active {
        color: white;
        background-color: #404040;
      }

      [data-bs-theme=dark] .back-to-top-alt {
        color: black;
        background-color: white;
      }

      [data-bs-theme=dark] .back-to-top-alt:hover {
        color: black;
        background-color: #f2f2f2;
      }

      [data-bs-theme=dark] .back-to-top-alt:active {
        color: black;
        background-color: #bfbfbf;
      }

      img.atproducts-image {
        min-width: 200px;
        width: 40%;
      }

      img.toucan-image {
        min-width: 200px;
        width: 30%;
      }

      .atproducts-background {
        background-image: url(/media/images/hero.webp);
        background-size: cover !important;
        background-position: center center !important;
        background-repeat: no-repeat !important;
      }

      .toucan-background {
        background-image: url(/media/images/toucanbanner.png);
        background-size: cover !important;
        background-position: center center !important;
        background-repeat: no-repeat !important;
      }

      .d-initial {
        display: initial;
      }

      .custom-br {
        height: 5rem !important;
      }

      .filter {
        filter: drop-shadow(2px 4px 6px black);
      }

      .cover-1 {
        background-color: rgba(0, 0, 0, 0.1);
      }

      .cover-2 {
        background-color: rgba(0, 0, 0, 0.2);
      }

      .cover-3 {
        background-color: rgba(0, 0, 0, 0.3);
      }

      .cover-5 {
        background-color: rgba(0, 0, 0, 0.5);
      }

      p:last-of-type {
        margin-bottom: 0 !important;
      }

      @media (min-width: 349.98px) {
        img.atproducts-image {
          min-width: 250px;
        }

        img.toucan-image {
          min-width: 250px;
        }
      }

      @media (min-width: 767.98px) {
        p:has(+ div#tpt2-album) {
          margin-bottom: 0 !important;
          padding-bottom: 1rem;
        }

        img.atproducts-image {
          min-width: 290px;
        }
      }

      .link-custom {
        color: RGBA(var(--bs-dark-rgb, var(--bs-link-opacity, 1)));
        -webkit-text-decoration-color: RGBA(var(--bs-dark-rgb), var(--bs-link-underline-opacity, 1));
        text-decoration-color: RGBA(var(--bs-dark-rgb), var(--bs-link-underline-opacity, 1));
      }
      .link-custom:hover, .link-custom:focus {
        color: RGBA(26, 30f, 33, var(--bs-link-opacity, 1));
        -webkit-text-decoration-color: RGBA(26, 30, 33, var(--bs-link-underline-opacity, 1));
        text-decoration-color: RGBA(26, 30, 33, var(--bs-link-underline-opacity, 1));
      }

      [data-bs-theme=dark] .link-custom {
        color: #adb5bd !important;
        -webkit-text-decoration-color: RGBA(var(--bs-dark-rgb), var(--bs-link-underline-opacity, 1));
        text-decoration-color: RGBA(var(--bs-dark-rgb), var(--bs-link-underline-opacity, 1));
